post command for the api

const axios = require('axios')

axios.post('http://shorty-api-shortlink.herokuapp.com/api/url/shorten', {
        longUrl: 'http://facebook.com',
        "token": "//provide by the login//"
    })
    .then(function(response) {
        console.log(response);
        console.log(response.data)
    })
    .catch(function(error) {
        console.log(error);
    });



axios.post('http://shorty-api-shortlink.herokuapp.com/api/user/login', {

        "email": "elyaschaimi@hotmail.fr",
        "password": "freebox1"
    })
    .then(function(response) {
        console.log(response);
        console.log(response.data)
    })
    .catch(function(error) {
        console.log(error);
    });


